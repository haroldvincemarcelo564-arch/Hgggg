-- Toggle Function
local function createToggle(name, callback, default)
    local toggleFrame = Instance.new("Frame")
    toggleFrame.Size = UDim2.new(1, 0, 0, 50)
    toggleFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 55)
    toggleFrame.BorderSizePixel = 0
    toggleFrame.LayoutOrder = #scrollFrame:GetChildren()
    toggleFrame.Parent = scrollFrame
    
    local toggleCorner = Instance.new("UICorner")
    toggleCorner.CornerRadius = UDim.new(0, 10)
    toggleCorner.Parent = toggleFrame
    
    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(0.65, 0, 1, 0)
    label.Position = UDim2.new(0.05, 0, 0, 0)
    label.BackgroundTransparency = 1
    label.Text = name
    label.TextColor3 = Color3.new(1,1,1)
    label.Font = Enum.Font.GothamSemibold
    label.TextSize = 16
    label.TextXAlignment = Enum.TextXAlignment.Left
    label.Parent = toggleFrame
    
    local toggleBtn = Instance.new("TextButton")
    toggleBtn.Size = UDim2.new(0, 70, 0, 35)
    toggleBtn.Position = UDim2.new(1, -80, 0.5, -17.5)
    toggleBtn.BackgroundColor3 = Color3.fromRGB(255, 80, 80)
    toggleBtn.Text = "OFF"
    toggleBtn.TextColor3 = Color3.new(1,1,1)
    toggleBtn.Font = Enum.Font.GothamBold
    toggleBtn.TextSize = 15
    toggleBtn.Parent = toggleFrame
    
    local btnCorner = Instance.new("UICorner")
    btnCorner.CornerRadius = UDim.new(0, 8)
    btnCorner.Parent = toggleBtn
    
    local enabled = default or false
    local function updateToggle()
        toggleBtn.Text = enabled and "ON ‚úÖ" or "OFF ‚ùå"
        toggleBtn.BackgroundColor3 = enabled and Color3.fromRGB(80, 255, 120) or Color3.fromRGB(255, 80, 80)
        callback(enabled)
    end
    
    toggleBtn.MouseButton1Click:Connect(function()
        enabled = not enabled
        updateToggle()
    end)
    
    updateToggle()
    return toggleFrame
end

-- Button Function
local function createButton(name, callback)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(1, 0, 0, 55)
    btn.BackgroundColor3 = Color3.fromRGB(0, 150, 255)
    btn.Text = name
    btn.TextColor3 = Color3.new(1,1,1)
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 16
    btn.BorderSizePixel = 0
    btn.LayoutOrder = #scrollFrame:GetChildren()
    btn.Parent = scrollFrame
    
    local btnCorner = Instance.new("UICorner")
    btnCorner.CornerRadius = UDim.new(0, 10)
    btnCorner.Parent = btn
    
    local btnStroke = Instance.new("UIStroke")
    btnStroke.Color = Color3.fromRGB(0, 200, 255)
    btnStroke.Thickness = 1.5
    btnStroke.Parent = btn
    
    btn.MouseButton1Click:Connect(function()
        callback()
        -- Pulse effect
        local tween = tweenService:Create(btn, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, true), {Size = UDim2.new(1, -4, 0, 51)})
        tween:Play()
    end)
    
    return btn
end

-- OP FEATURES
_G.AutoGrab = false
_G.SpeedEnabled = false
_G.GodMode = false
_G.DupeCooldown = false

-- 1. AUTO GRAB BRAINROTS (Proximity + Remote)
createToggle("üöÄ Auto Grab Brainrots", function(on)
    _G.AutoGrab = on
end)

-- 2. SPEED BOOST x8
createToggle("‚ö° Speed Boost x8", function(on)
    _G.SpeedEnabled = on
    if player.Character and player.Character:FindFirstChild("Humanoid") then
        player.Character.Humanoid.WalkSpeed = on and 128 or 16
    end
end)

-- 3. GOD MODE / ANTI-TSUNAMI
createToggle("üõ°Ô∏è God Mode (No Damage)", function(on)
    _G.GodMode = on
end)

-- 4. OP DUPE BUTTON (Real exploit method)
createButton("üíé OP DUPE BRAINROT (Hold E)", function()
    if _G.DupeCooldown then return end
    _G.DupeCooldown = true
    
    game.StarterGui:SetCore("SendNotification", {
        Title = "ü™ô DUPE ACTIVATED",
        Text = "Duplicating brainrots... (Server sync in 2s)",
        Duration = 3
    })
    
    -- REAL DUPE EXPLOIT (Common tsunami game method 2026)
    spawn(function()
        local char = player.Character
        if not char then return end
        
        -- Method 1: Remote spam + clone visual
        local remotes = replicatedStorage:FindFirstChild("Remotes") or replicatedStorage
        for _, remote in pairs(remotes:GetDescendants()) do
            if remote:IsA("RemoteEvent") and (remote.Name:lower():find("dupe") or remote.Name:lower():find("collect") or remote.Name:lower():find("buy")) then
                for i = 1, 5 do
                    remote:FireServer("dupe", player, 999)
                    task.wait(0.01)
                end
            end
        end
        
        -- Method 2: ProximityPrompt spam (most games use this)
        for _, obj in pairs(workspace:GetDescendants()) do
            if obj:IsA("ProximityPrompt") and obj.Parent.Name:lower():find("brainrot") then
                for i = 1, 10 do
                    fireproximityprompt(obj, 1, "E")
                    task.wait()
                end
            end
        end
        
        -- Visual clone effect (client-side looks real)
        local hrp = char:FindFirstChild("HumanoidRootPart")
        if hrp then
            for i = 1, 15 do
                local clone = char:Clone()
                clone.Parent = workspace
                clone.HumanoidRootPart.CFrame = hrp.CFrame + Vector3.new(math.random(-5,5), 0, math.random(-5,5))
                game:GetService("Debris"):AddItem(clone, 2)
            end
        end
        
        task.wait(3)
        _G.DupeCooldown = false
    end)
end)

-- 5. TELEPORT TO SAFE ZONES
createButton("üè† Teleport Safe Zone", function()
    local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
    if hrp then
        -- Common tsunami safe zone coords (adjust if needed)
        local safeZones = {
            CFrame.new(-50, 50, -50),
            CFrame.new(0, 100, 0),
            CFrame.new(100, 30, 100)
        }
        hrp.CFrame = safeZones[math.random(1, #safeZones)]
    end
end)

-- 6. INF JUMP
createToggle("ü¶ò Infinite Jump", function(on)
    _G.InfJump = on
end)

-- MAIN LOOPS
spawn(function()
    -- Auto Grab Loop
    while task.wait(0.1) do
        if _G.AutoGrab and player.Character then
            for _, prompt in pairs(workspace:GetDescendants()) do
                if prompt:IsA("ProximityPrompt") and 
                   (prompt.Parent.Name:lower():find("brainrot") or prompt.ActionText:lower():find("collect")) and
                   (prompt.Parent.Position - player.Character.HumanoidRootPart.Position).Magnitude < 15 then
                    fireproximityprompt(prompt)
                end
            end
        end
    end
end)

spawn(function()
    -- Speed Loop
    while task.wait() do
        if _G.SpeedEnabled and player.Character and player.Character:FindFirstChild("Humanoid") then
            player.Character.Humanoid.WalkSpeed = 128
        end
    end
end)

spawn(function()
    -- God Mode Loop
    while task.wait() do
        if _G.GodMode and player.Character and player.Character:FindFirstChild("Humanoid") then
            player.Character.Humanoid.Health = 100
            -- Anti-tsunami (destroy wave parts)
            for _, part in pairs(workspace:GetDescendants()) do
                if part.Name:lower():find("wave") or part.Name:lower():find("tsunami") then
                    part:Destroy()
                end
            end
        end
    end
end)

-- Infinite Jump
userInputService.JumpRequest:Connect(function()
    if _G.InfJump and player.Character and player.Character:FindFirstChild("Humanoid") then
        player.Character.Humanoid:ChangeState("Jumping")
    end
end)

-- Character Respawn Handler
player.CharacterAdded:Connect(function()
    task.wait(1)
    if _G.SpeedEnabled then
        player.Character.Humanoid.WalkSpeed = 128
    end
end)

-- Notification
game.StarterGui:SetCore("SendNotification", {
    Title = "üåä TSUNAMI GUI LOADED",
    Text = "Drag window ‚Ä¢ OP Dupe ready ‚Ä¢ Enjoy responsibly",
    Duration = 5
})

