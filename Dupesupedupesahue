-- üî• EQUIPPED ITEM DUPE BUTTON
makeButton("üî• DUPE EQUIPPED ITEM (Infinite Tools)", function()
    spawn(function()
        local character = player.Character
        if not character then return end
        
        -- Find equipped tool
        local equippedTool = character:FindFirstChildOfClass("Tool")
        if not equippedTool then
            game.StarterGui:SetCore("SendNotification",{
                Title="‚ùå NO TOOL", Text="Equip a brainrot/tool first!", Duration=3
            })
            return
        end
        
        game.StarterGui:SetCore("SendNotification",{
            Title="üî• DUPE STARTED", Text="Cloning " .. equippedTool.Name .. " x50...", Duration=4
        })
        
        -- DUPE LOOP: Clone equipped ‚Üí move to backpack ‚Üí repeat
        for i = 1, 50 do
            local clone = equippedTool:Clone()
            clone.Parent = backpack
            -- Auto-equip first clone (makes it look infinite)
            if i == 1 then
                clone.Parent = character
            end
            task.wait(0.05)
        end
        
        -- Remote sell spam for any sell remotes
        for _, rem in pairs(replicatedStorage:GetDescendants()) do
            if rem:IsA("RemoteEvent") and rem.Name:lower():find("sell") then
                for j = 1, 10 do
                    rem:FireServer(equippedTool.Name, 999)
                    task.wait(0.01)
                end
            end
        end
        
        game.StarterGui:SetCore("SendNotification",{
            Title="‚úÖ " .. equippedTool.Name .. " DUPED", 
            Text="50x cloned + auto-equipped! Sell for cash", 
            Duration=5
        })
    end)
end)

