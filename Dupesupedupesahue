    for _, part in pairs(workspace:GetDescendants()) do
        if part.Name:lower():find("wave") or part.Name:lower():find("tsunami") or 
           (part.Name:lower():find("wall") and part.Parent.Name:find("VIP")) then
            part:Destroy()
        end
    end
end)

-- MAIN LOOPS (Heartbeat = anti-detection)
local connections = {}
connections.auto = runService.Heartbeat:Connect(function()
    if _G.AutoCollect and player.Character then
        for _, prompt in pairs(workspace:GetDescendants()) do
            if prompt:IsA("ProximityPrompt") and prompt.ActionText:lower():find("collect") and 
               (prompt.Parent.Position - player.Character.HumanoidRootPart.Position).Magnitude < 100 then
                fireproximityprompt(prompt)
            end
        end
    end
end)

connections.speed = runService.Heartbeat:Connect(function()
    if _G.SpeedHack and player.Character then
        local hum = player.Character:FindFirstChild("Humanoid")
        if hum then hum.WalkSpeed = 200 end
        local leaderstats = player:FindFirstChild("leaderstats")
        if leaderstats then
            local speedStat = leaderstats:FindFirstChild("Speed")
            if speedStat then speedStat.Value = 999 end
        end
    end
end)

connections.god = runService.Heartbeat:Connect(function()
    if _G.GodMode and player.Character then
        local hum = player.Character:FindFirstChild("Humanoid")
        if hum then 
            hum.PlatformStand = true
            hum.Health = 100
        end
        -- Live wave delete
        for _, part in pairs(workspace:GetDescendants()) do
            if part.Name:lower():find("wave") or part.Name:lower():find("tsunami") then
                part:Destroy()
            end
        end
    end
end)

