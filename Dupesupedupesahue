-- TOGGLE FUNC
local function makeToggle(name, callback, default)
    local togFrame = Instance.new("Frame")
    togFrame.Size = UDim2.new(1,0,0,55)
    togFrame.BackgroundColor3 = Color3.fromRGB(40,40,55)
    togFrame.Parent = scroll
    local togCorner = Instance.new("UICorner")
    togCorner.CornerRadius = UDim.new(0,10)
    togCorner.Parent = togFrame
    
    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(0.7,0,1,0)
    label.BackgroundTransparency = 1
    label.Text = name
    label.TextColor3 = Color3.new(1,1,1)
    label.Font = Enum.Font.GothamSemibold
    label.TextSize = 16
    label.TextXAlignment = Enum.TextXAlignment.Left
    label.Parent = togFrame
    
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(0,80,0,40)
    btn.Position = UDim2.new(1,-90,0.5,-20)
    btn.BackgroundColor3 = Color3.fromRGB(255,80,80)
    btn.Text = "OFF ‚ùå"
    btn.TextColor3 = Color3.new(1,1,1)
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 14
    btn.Parent = togFrame
    local btnCorner = Instance.new("UICorner")
    btnCorner.CornerRadius = UDim.new(0,8)
    btnCorner.Parent = btn
    
    local state = default or false
    local function update()
        state = not state
        btn.Text = state and "ON ‚úÖ" or "OFF ‚ùå"
        btn.BackgroundColor3 = state and Color3.fromRGB(80,255,120) or Color3.fromRGB(255,80,80)
        callback(state)
    end
    btn.MouseButton1Click:Connect(update)
    return state
end

-- BUTTON FUNC
local function makeButton(name, callback)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(1,0,0,60)
    btn.BackgroundColor3 = Color3.fromRGB(0,150,255)
    btn.Text = name
    btn.TextColor3 = Color3.new(1,1,1)
    btn.Font = Enum.GothamBold
    btn.TextSize = 16
    btn.Parent = scroll
    local btnCorner = Instance.new("UICorner")
    btnCorner.CornerRadius = UDim.new(0,12)
    btnCorner.Parent = btn
    local btnStroke = Instance.new("UIStroke")
    btnStroke.Color = Color3.fromRGB(0,200,255)
    btnStroke.Thickness = 2
    btnStroke.Parent = btn
    
    btn.MouseButton1Click:Connect(function()
        callback()
        local pulse = tweenService:Create(btn, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, true), {Size = UDim2.new(1,-6,0,56)})
        pulse:Play()
    end)
end

